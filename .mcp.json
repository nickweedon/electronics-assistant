{
  "mcpServers": {
    "playwright-mcp-server": {
      "comment": "Playwright MCP Server",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/mnt/c/docker/blob-storage:/mnt/blob-storage",
        "-e",
        "BLOB_STORAGE_ROOT=/mnt/blob-storage",
        "-e",
        "BLOB_MAX_SIZE_MB=2048",
        "-e",
        "BLOB_TTL_HOURS=24",
        "-e",
        "BLOB_SIZE_THRESHOLD_KB=50",
        "-e",
        "BLOB_CLEANUP_INTERVAL_MINUTES=60",
        "-e",
        "PLAYWRIGHT_BROWSER=chrome",
        "-e",
        "PLAYWRIGHT_HEADLESS=true",
        "-e",
        "PLAYWRIGHT_NO_SANDBOX=true",
        "-e",
        "PLAYWRIGHT_TIMEOUT_ACTION=15000",
        "-e",
        "PLAYWRIGHT_TIMEOUT_NAVIGATION=5000",
        "-e",
        "PLAYWRIGHT_VIEWPORT_SIZE=1920x1080",
        "-e",
        "PROXY_DEBUG=true",
        "playwright-proxy-mcp:latest",
        "uv",
        "run",
        "playwright-proxy-mcp"
      ]
    },
    "playwright-real-session-mcp-server": {
      "comment": "Playwright MCP Server for use with browser and a real user session. Do not use this unless explicitly asked to.",
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/opt/src/mcp/playwright-proxy-mcp",
        "playwright-proxy-mcp"
      ],
      "env": {
        "BLOB_STORAGE_ROOT": "/mnt/c/docker/blob-storage",
        "BLOB_MAX_SIZE_MB": "2048",
        "BLOB_TTL_HOURS": "24",
        "BLOB_SIZE_THRESHOLD_KB": "50",
        "BLOB_CLEANUP_INTERVAL_MINUTES": "60",
        "PLAYWRIGHT_USER_DATA_DIR": "c:/tools/playwright/playwright-output",
        "PLAYWRIGHT_OUTPUT_DIR": "c:/tools/playwright/mcp-output",
        "PLAYWRIGHT_STORAGE_STATE": "c:/tools/playwright/browser-profile/state.json",
        "PLAYWRIGHT_TIMEOUT_ACTION": "15000",
        "PLAYWRIGHT_TIMEOUT_NAVIGATION": "5000",
        "PLAYWRIGHT_SAVE_SESSION": "true",
        "PLAYWRIGHT_SAVE_TRACE": "true",
        "PLAYWRIGHT_BROWSER": "chrome",
        "PLAYWRIGHT_WSL_WINDOWS": "true"
      }
    },
    "digikey": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-p",
        "8139",
        "-v",
        "/mnt/c/docker/digikey-mcp-env:/workspace/.env:ro",
        "-v",
        "/mnt/c/docker/digikey-localhost-key.pem:/workspace/localhost-key.pem:ro",
        "-v",
        "/mnt/c/docker/digikey-localhost-cert.pem:/workspace/localhost-cert.pem:ro",
        "-v",
        "/mnt/c/docker/digikey-tokens:/workspace/.digikey_tokens",
        "-v",
        "/var/run/docker.sock:/var/run/docker.sock:ro",
        "digikey-mcp:latest",
        "/bin/bash",
        "-c",
        "cd /workspace && uv run digikey_mcp_server.py"
      ]
    },
    "partsbox": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/mnt/c/docker/partsbox-mcp-env:/workspace/.env:ro",
        "-v",
        "/mnt/c/docker/blob-storage:/mnt/blob-storage",
        "-e",
        "PARTSBOX_BLOB_STORAGE_ROOT=/mnt/blob-storage",
        "-e",
        "PARTSBOX_BLOB_STORAGE_MAX_SIZE_MB=2048",
        "-e",
        "PARTSBOX_BLOB_STORAGE_TTL_HOURS=24",
        "partsbox-mcp:latest",
        "uv",
        "run",
        "partsbox-mcp"
      ]
    },
    "google-docs": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-p",
        "3000",
        "-v",
        "/mnt/c/docker/google-docs-mcp:/workspace/credentials",
        "-v",
        "/mnt/c/docker/blob-storage:/mnt/blob-storage",
        "-v",
        "/var/run/docker.sock:/var/run/docker.sock:ro",
        "-e",
        "BLOB_STORAGE_ROOT=/mnt/blob-storage",
        "-e",
        "BLOB_STORAGE_MAX_SIZE_MB=2048",
        "-e",
        "BLOB_STORAGE_TTL_HOURS=24",
        "google-docs-mcp:latest",
        "uv",
        "run",
        "google-docs-mcp"
      ]
    },
    "farnell-mcp": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        "/mnt/c/docker/farnell-mcp/.env",
        "farnell-mcp:latest"
      ]
    },
    "mouser": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        "/mnt/c/docker/mouser-mcp/.env",
        "mouser-mcp:latest"
      ]
    },
    "mcp-resource-server": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-v",
        "/mnt/c/docker/blob-storage:/mnt/blob-storage",
        "-e",
        "RESOURCE_SERVER_DEBUG=true",
        "-e",
        "RESOURCE_SERVER_MASK_ERRORS=false",
        "-e",
        "BLOB_STORAGE_ROOT=/mnt/blob-storage",
        "-e",
        "BLOB_MAX_SIZE_MB=100",
        "-e",
        "BLOB_TTL_HOURS=24",
        "-e",
        "HOST_BLOB_STORAGE_ROOT=/mnt/c/docker/blob-storage",
        "mcp-resource-server:latest"
      ]
    }
  }
}